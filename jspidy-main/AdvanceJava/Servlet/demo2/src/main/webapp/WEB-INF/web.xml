<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-class>demo2.SubServlet</servlet-class>
  	<servlet-name>ss</servlet-name>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ss</servlet-name>
  	<url-pattern>/Subtra</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-class>demo2.DetailsServlet</servlet-class>
  	<servlet-name>dss</servlet-name>
  </servlet>
  <servlet-mapping>
  	<servlet-name>dss</servlet-name>
  	<url-pattern>/print</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-class>demo2.RegisterData</servlet-class>
  	<servlet-name>reg</servlet-name>
  </servlet>
  <servlet-mapping>
  	<servlet-name>reg</servlet-name>
  	<url-pattern>/PrintData</url-pattern>
  </servlet-mapping>
</web-app>
